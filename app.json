{
  "expo": {
    "name": "AvayLable  Alpha",
    "slug": "avaylablealpha",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "avaylablelocusalpha",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "splash": {
      "image": "./assets/splash/splashscreen_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.neighborhub11.avaylablealpha",
      "buildNumber": "1",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app uses location to show nearby services and navigation.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app uses location to show nearby services and navigation.",
        "NSCameraUsageDescription": "This app uses camera to take profile pictures.",
        "NSPhotoLibraryUsageDescription": "This app uses photo library to select profile pictures."
      }
    },
    "android": {
      "versionCode": 1,
      "package": "com.neighborhub11.avaylablealpha",
      "permissions": [
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBP5Pw0wbGjXMjYQTO4wk5GRZY3iyMFXJg"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://avaylable.com/"
        }
      ],
      [
        "@react-native-google-signin/google-signin"
      ],
      [
      "expo-location",
      {
        "locationAlwaysAndWhenInUsePermission": "This app uses location to show nearby services and navigation.",
        "locationWhenInUsePermission": "This app uses location to show nearby services and navigation.",
        "isIosBackgroundLocationEnabled": false,
        "isAndroidBackgroundLocationEnabled": false
      }
    ],
      "expo-camera",
      "expo-image-picker"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "96ea1fe4-ba89-41eb-97ee-4601879e3715"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/96ea1fe4-ba89-41eb-97ee-4601879e3715"
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}